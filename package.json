{
  "name": "naxui-icons",
  "version": "1.0.4",
  "description": "Google Material icons distributed as SVG React components.",
  "keywords": ["Material icons", "Google Material icons", "SVG icons"],
  "author": "Naxrul Ahmed",
  "license": "ISC",
  "scripts": {
    "start": "parcel index.html --out-dir public",
    "tsc": "tsc --module CommonJS --outDir dist",
    "move": "cp package.json dist/package.json && cp readme.md dist/readme.md",
    "generate": "node generate.js",
    "build": "npm run generate && npm run tsc && npm run move"
  },
  "devDependencies": {
    "@material-design-icons/svg": "^0.14.8",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.14",
    "node-html-parser": "^6.1.5",
    "react": "^18.2.0",
    "typescript": "^4.9.5",
    "parcel": "^1.12.3",
    "react-app-polyfill": "^3.0.0"
  },
  "dependencies": {
    "naxui-manager": "latest"
  }
}
